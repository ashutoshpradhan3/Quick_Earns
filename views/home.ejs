    <script>

        $(document).ready(function () {
            var timer; // Variable to store the timer ID
        
            // Show dropdown menu when mouse enters "Jobs" button or dropdown
            $('#jobs, #jobs-dropdown').mouseover(function () {
                // Clear any existing timer
                clearTimeout(timer);
                $('#jobs-dropdown').show();
            });
        
            // Hide dropdown menu when mouse leaves "Jobs" button or dropdown
            $('#jobs, #jobs-dropdown').mouseleave(function () {
                // Set a timer to hide the dropdown after 500 milliseconds
                timer = setTimeout(function() {
                    $('#jobs-dropdown').hide();
                }, 500);
            });
        });
        
           
        

